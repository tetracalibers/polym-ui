{
  "name": "react-tsx-props",
  "version": "0.0.0",
  "description": "React props definition helper tool with TypeScript",
  "author": {
    "name": "tomixy",
    "url": "https://tetracalibers.notion.site/tetracalibers/tomixy-TetraCalibers-30b94fb9fc054d4da667539ef35f42c6"
  },
  "homepage": "https://github.com/tetracalibers/React-polyhexUI/tree/main/packages/core#readme",
  "license": "MIT",
  "type": "module",
  "types": "lib/@types/index.d.ts",
  "exports": {
    "./util-types": {
      "import": "./lib/util-types/index.es.js",
      "require": "./lib/util-types/index.cjs",
      "node": "./lib/util-types/index.es.js",
      "default": "./lib/util-types/index.cjs"
    },
    ".": {
      "import": "./lib/def-tools/index.es.js",
      "require": "./lib/def-tools/index.cjs",
      "node": "./lib/def-tools/index.es.js",
      "default": "./lib/def-tools/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tetracalibers/React-polyhexUI.git"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "bundle:util-types": "tsc --noEmit --rootDir=src/util-types && rollup --config rollup.util-types.config.js",
    "bundle:def-tool": "tsc --noEmit --rootDir=src/def-tool' && rollup --config rollup.def-tool.config.js",
    "prebuild": "rimraf lib",
    "build": "yarn bundle:util-types && yarn bundle:def-tool",
    "start": "yarn build && node lib/bundle.cjs"
  },
  "bugs": {
    "url": "https://github.com/tetracalibers/React-polyhexUI/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/node": "^18.7.3",
    "babel-loader": "^8.2.5",
    "csstype": "^3.1.0",
    "esbuild": "^0.14.48",
    "fp-ts": "^2.12.2",
    "lodash": "^4.17.21",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.7",
    "rollup-plugin-esbuild": "^4.9.1",
    "ts-node": "^10.9.1",
    "ts-typedef-helper": "*",
    "typescript": "^4.7.4",
    "zx": "^7.0.8"
  }
}
